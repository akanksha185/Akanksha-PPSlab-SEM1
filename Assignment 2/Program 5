#include<stdio.h>
void main()
{
    char ch;
    printf("Enter any character");
    scanf("%c",&ch);
    if (ch=='a'|| ch=='A'|| ch=='e'|| ch=='E'|| ch=='i'|| ch=='I'|| ch=='o'|| ch=='O'|| ch=='u'|| ch=='U'||)
    {
        printf("%c is vowel",ch);break;
    }
    esle if ((ch>='a'&&ch<=='z'||(ch>='A'&&ch<='z')))
    {
        printf("%c is consotant",ch);break;
    }
    else
    {
        printf("%c is not an alphabet",ch);
    }
}