#include<stdio.h>
void main() 
{
    char ch;
    printf("Enter any character:");
    scanf("%c",&ch);
    if ((ch>='a' && ch<='z'|| (ch>='A'&& ch<='z')))
    {
        printf("This is an alphabet.\n");break;
    }
    else if (ch>='0'&& ch<=='9')
    {
        printf("This is a digit .\n");break;
    }
    else 
    {
        printf("This is a special character.\n");
    }
}  
